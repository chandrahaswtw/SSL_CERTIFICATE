<table class="table table-hover shadow p-3 mb-5 bg-white rounded" id="userTable">
  <caption>LIST OF USERS</caption>
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Username</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.username}}</td>
      <td>
        <i class="fas fa-trash-alt"></i>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>